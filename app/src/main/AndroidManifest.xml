<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rajbirbhinder.app_work" >

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="Women Security App"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="Women Security App" >
        </activity>
        <activity
            android:name=".AddNumbers"
            android:label="@string/title_activity_add_numbers" >
        </activity>
        <activity
            android:name=".WomenSecurityApp"
            android:label="@string/title_activity_women_security_app" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Tips"
            android:label="@string/title_activity_tips" >
        </activity>
    </application>

</manifest>
